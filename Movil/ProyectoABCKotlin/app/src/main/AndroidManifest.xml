        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProyectoABCKotlin"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".registroCandidato.VerEntrevistasProgramadasActivity"
            android:exported="false" />
        <activity
            android:name=".registroCandidato.VerResultadosEntrevistasActivity"
            android:exported="false" />
        <activity
            android:name=".desempenoEmpleado.RegistroDatDesempenoEmpleadoActivity"
            android:exported="false" />
        <activity
            android:name=".desempenoEmpleado.RegistroDesempenoEmpleadoActivity"
            android:exported="false" />

        <service
            android:name=".service.ApiDesempenoEmpleado"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name=".registroCandidato.VerResultadoPruebaActivity"
            android:exported="false" />

        <service
            android:name=".service.ApiPrueba"
            android:enabled="true"
            android:exported="true" />

